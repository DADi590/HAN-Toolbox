<!-- Main Items > Toolbox Settings > Toolbox Multi Language > Install Multi Language (here : lang available, english hardcoded) -->
		
		<View id="han_toolbox_install_lang_main"> 
		<Attributes>
			<Table key="han_toolbox_check_lang_item">
				<Pair key="icon"><String>/dev_hdd0/game/HANTOOLBX/USRDIR/IMAGES/list.png</String></Pair>
				<Pair key="title"><String>HAN Toolbox is translated in the following languages :</String></Pair>
				<Pair key="info"><String>--- None ATM ---</String></Pair>
			</Table>
			<Table key="han_download_lang_rco">
				<Pair key="icon"><String>/dev_hdd0/game/HANTOOLBX/USRDIR/IMAGES/pkg_file.png</String></Pair>
				<Pair key="title"><String>Step 1: Download lang RCO file</String></Pair>
				<Pair key="info"><String>Download explore_plugin_full.rco to HANTOOLBX/USRDIR/files/ ready for installation</String></Pair>
			</Table>
			<Table key="han_inject_lang_rco">
				<Pair key="icon"><String>/dev_hdd0/game/HANTOOLBX/USRDIR/IMAGES/inj_rco.png</String></Pair>
				<Pair key="title"><String>Step 2: Inject lang RCO file</String></Pair>
				<Pair key="info"><String>Copy downloaded explore_plugin_full.rco from HANTOOLBX/USRDIR/files to dev_flash/vsh/resource/*.rco</String></Pair>
				<Pair key="module_name"><String>webrender_plugin</String></Pair>
				<Pair key="module_action"><String>http://ps3xploit.com/flash_rco_replacer/flash_rco_replacer.html</String></Pair>
			</Table>
			<Table key="han_download_patch_file">
				<Pair key="icon"><String>/dev_hdd0/game/HANTOOLBX/USRDIR/IMAGES/pkg_file.png</String></Pair>
				<Pair key="title"><String>Step 3: Download and install patch PKG file</String></Pair>
				<Pair key="info"><String>Download and install patch PKG file and reboot after</String></Pair>
			</Table>
		</Attributes>
			<Items>
			<Query 
				class="type:x-xmb/module-action" 
				key="han_toolbox_check_lang_item" 
				attr="han_toolbox_check_lang_item"
				/>
			<Query 
				class="type:x-xmb/folder-pixmap" 
				key="han_download_lang_rco" 
				attr="han_download_lang_rco"
				src="xmb://localhost/dev_hdd0/game/HANTOOLBX/USRDIR/han_toolbox_stuff_downloader.xml#han_download_lang_rco_main"
				/>
			<Query 
				class="type:x-xmb/module-action"
				key="han_inject_lang_rco"
				attr="han_inject_lang_rco"
				/>
			<Query 
				class="type:x-xmb/folder-pixmap" 
				key="han_download_patch_file" 
				attr="han_download_patch_file"
				src="xmb://localhost/dev_hdd0/game/HANTOOLBX/USRDIR/han_toolbox_stuff_downloader.xml#han_download_patch_file_main"
				/>
			</Items> 
		</View>

