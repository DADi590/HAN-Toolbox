				<Pair key="title"><String>Toolbox Multi Language</String></Pair>
				<Pair key="info"><String>Install/Uninstall the HAN Toolbox lang resources files</String></Pair>
			</Table>
			<Table key="han_toolbox_resources">
				<Pair key="icon"><String>/dev_hdd0/game/HANTOOLBX/USRDIR/IMAGES/pkg_file.png</String></Pair>
