				<Pair key="info"><String>Install/Uninstall the HAN Toolbox resources package</String></Pair>
			</Table>
			<Table key="han_toolbox_update">
				<Pair key="icon"><String>http://raw.githubusercontent.com/ShaolinAssassin/HAN-Toolbox/master/update_status/v0.7-update.png</String></Pair>
				<Pair key="title"><String>Update Toolbox</String></Pair>
				<Pair key="info"><String>Download and install the latest Toolbox package and reboot after</String></Pair>
			</Table>
			<Table key="han_toolbox_about">
				<Pair key="icon"><String>/dev_hdd0/game/HANTOOLBX/USRDIR/IMAGES/list.png</String></Pair>
				<Pair key="title"><String>Version 0.7 (Full)</String></Pair>
