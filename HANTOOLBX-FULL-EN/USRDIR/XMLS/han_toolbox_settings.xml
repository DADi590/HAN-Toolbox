				class="type:x-xmb/folder-pixmap" 
				key="han_download_multi_lang_pkg" 
				attr="han_download_multi_lang_pkg"
				src="xmb://localhost/dev_hdd0/game/HANTOOLBX/USRDIR/XMLS/han_toolbox_stuff_downloader.xml#han_download_multi_lang_pkg_main"
				/>
			<Query 
				class="type:x-xmb/module-action"
				key="han_inject_lang_rco"
				attr="han_inject_lang_rco"
				/>
		</Items>
