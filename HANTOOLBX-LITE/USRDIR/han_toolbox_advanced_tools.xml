				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="title"><String>File Injector</String></Pair>
				<Pair key="info"><String>Use for adding XMB mods to flash or other file operations (Be careful)</String></Pair>
			</Table>
			<Table key="han_dex_settings_injector">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="title"><String>DEX XMB Setup</String></Pair>
				<Pair key="info"><String>Add/Remove DEX settings etc (Be careful)</String></Pair>
			</Table>
		</Attributes>
			<Items>
			<Query 
				class="type:x-xmb/module-action"
				key="han_file_copier"
				attr="han_file_copier"
				/>
			<Query 
				class="type:x-xmb/folder-pixmap" 
				key="han_file_browser"
				attr="han_file_browser" 				
				src="#han_file_browser_items"
				/>				
			<Query 
				class="type:x-xmb/folder-pixmap" 
				key="han_file_injector"
				attr="han_file_injector" 				
				src="#han_file_injector_items"
				/>
			<Query 
				class="type:x-xmb/folder-pixmap" 
				key="han_dex_settings_injector"
				attr="han_dex_settings_injector" 				
				src="han_dex_settings_injector_items"
				/>
			</Items>
		</View>
	
