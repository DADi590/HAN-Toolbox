<!-- Main Items > Advanced Tools -->
		
		<View id="han_advanced_menu_main"> 
		<Attributes> 
			<Table key="han_file_copier">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="title"><String>File Copier</String></Pair>
				<Pair key="info"><String>Transfer files between your PS3 and USB flash drive</String></Pair>
				<Pair key="module_name"><String>webrender_plugin</String></Pair>
				<Pair key="module_action"><String>javascript:eval(atob(""));</String></Pair>
			</Table>
			<Table key="han_file_browser">
				<Pair key="icon_rsc"><String>tex_album_icon</String></Pair>
				<Pair key="title"><String>File Explorer</String></Pair>
				<Pair key="info"><String>Browse complete file system and delete files (Be careful)</String></Pair>
			</Table>
